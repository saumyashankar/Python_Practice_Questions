   "source": [
    "def find_vowels_from_string(string):\n",
    "    vowels = []\n",
    "    for char in string:\n",
    "        if char.lower() in ['a', 'e', 'i', 'o', 'u']:\n",
    "            vowels.append(char)\n",
    "    return vowels"
   ]
  },
